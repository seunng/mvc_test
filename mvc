CREATE TABLE test_info(
meiNum NUMBER PRIMARY KEY,
meiName VARCHAR2(100) NOT NULL,
meiPrice NUMBER NOT NULL,
meiDesc VARCHAR2(2000)
);

CREATE SEQUENCE sql_meiNum;

SELECT * FROM test_info;

private long meiNum;
private String meiName;
private long meiPrice;
private String meiDesc;

COMMIT;
